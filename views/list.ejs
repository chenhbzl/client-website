<div class="wrap" id="list" data-page="<%=data.page%>" data-type="<%=data.collection%>">
    <% for(var i=0; i<data.result.length; i++) {%>
    <div class="box" id="<%=data.collection%>">
        <div class="boxInner">
            <a href="/<%=data.collection%>/profile/<%= data.result[i].id%>" target="_blank">
                <img class="lazy" data-original="http://img.pretty-girl.top/<%= data.result[i].localSmallPicSrc%><%=data.smallPicRule%>">
            </a>
            <div class="titleBox"><%= data.result[i].title%></div>
        </div>
    </div>
    <% } %>
    <%if(!data.result.length){%>
        <h1 style="width:100px;pxtext-align: center;position: fixed;top: 48%;margin-left:-50px;left: 50%">亲!木有啦!</h1>
    <%}%>
</div>


<div class="nav nav_left">&lt;</div>
<div class="nav nav_right">&gt;</div>

