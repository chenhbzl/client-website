


<div class="wrap" id="list" data-page="<%=data.page%>">
    <% for(var i=0; i<data.result.length; i++) {%>
    <div class="box">
        <div class="boxInner">
            <a href="/profile/<%= data.result[i].id%>" target="_blank">
                <img class="lazy" data-original="http://pretty-girl.b0.upaiyun.com/<%= data.result[i].localSmallPicSrc%>!umei.pc.small">
            </a>
            <div class="titleBox"><%= data.result[i].title%></div>
        </div>
    </div>
    <% } %>
</div>

<style>
    .nav {
        font-size: 70px;
        color: #fff;
        position: fixed;
        top: 50%;
        width: 100px;
        height: 100px;
        line-height: 100px;
        text-align: center;
        background: rgba(0, 0, 0, .5);
        cursor: pointer;
    }
    .nav:hover {
        background: rgba(0, 0, 0, .8);
    }
    .nav_left {
        left: 0;
    }
    .nav_right {
        right: 0;
    }
</style>
<div class="nav nav_left">&lt;</div>
<div class="nav nav_right">&gt;</div>